{
  "name": "ecl-ssg",
  "version": "0.0.1",
  "private": true,
  "description": "ecl-ssg",
  "author": "planctus",
  "keywords": [
    "gatsby",
    "ssg",
    "ECL",
    "ecl-webcomponents"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run copy && gatsby develop",
    "build": "npm run copy && gatsby build",
    "serve": "gatsby serve",
    "clean": "run-p clean:*",
    "clean:gatsby": "gatsby clean",
    "clean:assets": "rimraf static/build",
    "copy": "run-s copy:*",
    "copy:webcomponents": "ncp node_modules/@ecl/ecl-webcomponents/dist/ecl-webcomponents/ static",
    "copy:styles-ec": "ncp node_modules/@ecl/preset-ec/dist/styles/optional/ static/",
    "copy:styles-eu": "ncp node_modules/@ecl/preset-eu/dist/styles/optional/ static/",
    "copy:cms": "ncp node_modules/@ecl/ecl-webcomponents/dist/ecl-webcomponents/ static/admin/ecl-webcomponents",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ecl/ckeditor5-ecl-webcomponents": "0.12.0",
    "@ecl/ecl-webcomponents": "0.12.0",
    "@ecl/ecl-webcomponents-react": "0.12.0",
    "@types/react-html-parser": "2.0.4",
    "gatsby": "5.12.9",
    "gatsby-plugin-babel-exclude": "2.0.0",
    "gatsby-plugin-i18n": "1.0.1",
    "gatsby-plugin-image": "3.12.3",
    "gatsby-plugin-manifest": "5.12.3",
    "gatsby-plugin-netlify-cms": "7.12.0",
    "gatsby-plugin-sharp": "5.12.3",
    "gatsby-source-filesystem": "5.12.1",
    "gatsby-transformer-remark": "6.12.3",
    "gatsby-transformer-sharp": "5.12.3",
    "netlify-cms-app": "^2.15.72",
    "netlify-cms-proxy-server": "1.3.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-html-parser": "2.0.2"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-react": "5.1.0",
    "@ecl/preset-ec": "3.11.0",
    "@ecl/preset-eu": "3.11.0",
    "@types/node": "20.8.10",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "create-react-class": "15.7.0",
    "fast-glob": "3.3.1",
    "fs-extra": "11.1.1",
    "gray-matter": "4.0.3",
    "i18next": "23.6.0",
    "i18next-browser-languagedetector": "7.1.0",
    "js-search": "2.0.1",
    "ncp": "2.0.0",
    "netlify-cli": "15.11.0",
    "npm-run-all": "4.1.5",
    "puppeteer": "20.8.0",
    "react-i18next": "13.3.1",
    "react-intl": "6.5.1",
    "react-loadable": "5.5.0",
    "react-router-dom": "6.17.0",
    "rimraf": "5.0.5",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "semver": ">=5.7.2",
    "word-wrap": "aashutoshrathi/word-wrap",
    "trim": ">=0.0.3",
    "socket.io-parser": ">=4.2.3"
  }
}
